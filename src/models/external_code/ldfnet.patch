+from torch import nn
+import os
+import torch
+
+from LDFNet import Net
+
+H = 512
+W = 1024
+N_CLASSES = 19
+
+criterion = nn.CrossEntropyLoss(reduction='mean', ignore_index=255)
+model = Net(N_CLASSES)
+model.eval()
+
+rgb = torch.rand(size=(1, 3, H, W), dtype=torch.float32)
+depth_luminance = torch.rand(size=(1, 2, H, W), dtype=torch.float32)
+
+out_dir = '../../onnx_models'
+os.makedirs(out_dir, exist_ok=True)
+onnx_file_path = os.path.join(out_dir, 'ldfnet.onnx')
+torch.onnx.export(model,
+                  (rgb, depth_luminance),
+                  onnx_file_path,
+                  export_params=True,
+                  input_names=['rgb', 'hha'],
+                  output_names=['output'],
+                  do_constant_folding=True,
+                  verbose=False,
+                  opset_version=10
+                  )
+print(f'exported {onnx_file_path}')
